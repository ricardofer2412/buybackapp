<p id="notice"><%= notice %></p>

<h1>Buy Backs</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Middle name</th>
      <th>Last name</th>
      <th>Address</th>
      <th>Address two</th>
      <th>City</th>
      <th>State</th>
      <th>Zip code</th>
      <th>Email</th>
      <th>Drive license</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @buy_backs.each do |buy_back| %>
      <tr>
        <td><%= buy_back.first_name %></td>
        <td><%= buy_back.middle_name %></td>
        <td><%= buy_back.last_name %></td>
        <td><%= buy_back.address %></td>
        <td><%= buy_back.address_two %></td>
        <td><%= buy_back.city %></td>
        <td><%= buy_back.state %></td>
        <td><%= buy_back.zip_code %></td>
        <td><%= buy_back.email %></td>
        <td><%= buy_back.drive_license %></td>
        <td><%= link_to 'Show', buy_back %></td>
        <td><%= link_to 'Edit', edit_buy_back_path(buy_back) %></td>
        <td><%= link_to 'Destroy', buy_back, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Buy Back', new_buy_back_path %>
